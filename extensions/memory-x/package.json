{
  "name": "@kakezh/memory-x",
  "version": "1.0.0",
  "description": "Framework-agnostic hierarchical memory system for AI agents. Works with OpenClaw, LangChain, or standalone.",
  "type": "module",
  "keywords": [
    "memory",
    "agent",
    "ai",
    "hierarchical",
    "knowledge-graph",
    "framework-agnostic",
    "openclaw",
    "langchain"
  ],
  "author": "Kakezh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kakezh/openclaw-memoryplus.git"
  },
  "bugs": {
    "url": "https://github.com/Kakezh/openclaw-memoryplus/issues"
  },
  "homepage": "https://github.com/Kakezh/openclaw-memoryplus#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./adapters/generic": {
      "types": "./dist/adapters/generic.d.ts",
      "import": "./dist/adapters/generic.js"
    },
    "./adapters/openclaw": {
      "types": "./dist/adapters/openclaw.d.ts",
      "import": "./dist/adapters/openclaw.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "sql.js": "^1.10.3"
  },
  "optionalDependencies": {
    "better-sqlite3": "^11.0.0"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.1.0",
    "openclaw": ">=2026.1.26"
  },
  "peerDependenciesMeta": {
    "@langchain/core": {
      "optional": true
    },
    "openclaw": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.10.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "tsup": {
    "entry": [
      "index.ts",
      "core/index.ts",
      "adapters/generic.ts",
      "adapters/openclaw.ts"
    ],
    "format": "esm",
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
